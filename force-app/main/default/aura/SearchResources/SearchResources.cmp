<aura:component
                implements="flexipage:availableForAllPageTypes,force:hasRecordId,force:appHostable,force:lightningQuickAction,flexipage:availableForRecordHome,forceCommunity:availableForAllPageTypes,lightning:isUrlAddressable"
                access="global" controller="SearchResourcesController">
    <aura:html tag="style">
        .slds-modal__container { min-width: 60vw; }
    </aura:html>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:attribute name="roleBasedOnResReq" type="String" />
    <aura:attribute name="spinner" type="boolean" default="false" />
    <aura:attribute name="isLocationAvailble" type="boolean" default="false" />
    <aura:attribute name="defultDate" type="String" />
    <aura:attribute name="detailsOfSelectedResReq" type="pse__Resource_Request__c" />
    <aura:attribute name="searchDetailObject" type="Object" />
    <aura:attribute name="roleList" type="String" />
    <aura:attribute name="showfilterLogicSkills" type="Boolean" default="false" />
    <aura:attribute name="showfilterLogicCertifications" type="Boolean" default="false" />
    <aura:attribute name="resourceRequestName" type="String" />
    <aura:attribute name="selectedResourceRequest" type="String" />
    <aura:attribute name="isSearchSkillsModalOpen" type="Boolean" default="False" />
    <aura:attribute name="isSearchCertificationssModalOpen" type="Boolean" default="False" />
    <aura:attribute name="isSearchResourceRequests" type="Boolean" default="False" />
    <aura:attribute name="isResReqTable" type="Boolean" default="False" />
    <aura:attribute name="displayDisabledNextButton" type="Boolean" default="false" />
    <aura:attribute name="assignHold" type="Boolean" default="false" />
    <aura:attribute name="assignValue" type="Boolean" default="false" />
    <aura:attribute name="holdValue" type="Boolean" default="false" />
    <aura:attribute name="conName" type="String" />
    <aura:attribute name="assignmentCheckMap" type="Map" />
    <aura:attribute name="data" type="Object" />
    <aura:attribute name="columns" type="List" />
    <aura:attribute name="data2" type="Object" />
    <aura:attribute name="columns2" type="List" />
    <aura:attribute name="data3" type="Object" />
    <aura:attribute name="columns3" type="List" />
    <!--Changes Start For Attribute For Displaying Contacts-->
    <aura:attribute name="data4" type="Object" />
    <aura:attribute name="columns4" type="List" />
    <aura:attribute name="isDisplayingContacts" type="Boolean" default="False" />
    <aura:attribute name="page" type="Integer" description="using for store page Number" default="0"/>
    <aura:attribute name="pages" type="Integer" description="using for store All Pages page Number" default="0"/>
    <aura:attribute name="total" type="Integer" description="total records count store " default="0"/>
    <!--Changes End For Attribute For Displaying Contacts-->
    <aura:attribute name="lstSkill" type="Object[]" />
    <aura:attribute name="lstCertification" type="Object[]" />
    <aura:attribute name="skillSelectedRowsId" type="List" />
    <aura:attribute name="certificationSelectedRowsId" type="List" />
    <!--Start For Scheduling Strategies-->
    <!--<aura:attribute name="resourceRequestBasedOnSearch" type="pse__Resource_Request__c" />-->
    <aura:attribute name="mapStrategyBasedOnSchdulePattern" type="Map" />
    <aura:attribute name="scheduleRecord" type="pse__Schedule__c" />
    <aura:attribute name="startendDateBasisOfSchedulingPattern" type="Boolean" default="false" />
    <aura:attribute name="displayForWeeklyHours" type="Boolean" default="false" />
    <aura:attribute name="customHoursSchedule" type="Boolean" default="false" />
    <aura:attribute name="resId" type="Id" />
    <aura:attribute name="keyPattern" type="String" />
    <aura:attribute name="valueAssignHold" type="String" />
    <aura:attribute name="totalHours" type="Decimal" />
    <!--End For Scheduling Strategies-->
    <!-- Start For Map -->
    <aura:attribute name="mapMarkers" type="Object" />
    <aura:attribute name="zoomLevel" type="Integer" />
    <aura:attribute name="mapOpenClose" type="Boolean" default="false" />
    <!--End For Map-->
    <!--Start For Region, Practice and Group-->
    <aura:attribute name="isSearchPracticeModal" type="Boolean" default="false" />
    <aura:attribute name="isPracticeTable" type="Boolean" default="false" />
    <aura:attribute name="practiceName" type="String" />
    <aura:attribute name="isSearchRegionModal" type="Boolean" default="false" />
    <aura:attribute name="isRegionTable" type="Boolean" default="false" />
    <aura:attribute name="regionName" type="String" />
    <aura:attribute name="dataReg" type="Object" />
    <aura:attribute name="columnsReg" type="List" />
    <aura:attribute name="dataPrac" type="Object" />
    <aura:attribute name="columnsPrac" type="List" />
    <aura:attribute name="groupName" type="String" />
    <aura:attribute name="dataGroup" type="Object" />
    <aura:attribute name="columnsGroup" type="List" />
    <aura:attribute name="isSearchGroupModal" type="Boolean" default="false" />
    <aura:attribute name="isGroupTable" type="Boolean" default="false" />
    <!--End For Region and Practice-->
    <aura:attribute name="resReqAvailability" type="Boolean" default="false" />
    <aura:attribute name="toggleDropdown" type="String" default="" />
    <aura:attribute name="sortDirection" type="String" default="asc" />
    <aura:attribute name="defaultSortDirection" type="String" default="asc" />
    <aura:attribute name="sortedBy" type="String" />
    <aura:attribute name="regionDisabled" type="Boolean" default="false" />
    <aura:attribute name="practiceDisabled" type="Boolean" default="false" />
    <aura:attribute name="groupDisabled" type="Boolean" default="false" />
    <aura:attribute name="costDisabled" type="Boolean" default="false" />
    <aura:attribute name="utilizationDisabled" type="Boolean" default="false" />
    <aura:attribute name="searchOutResourcesPagePagesValue" type="Boolean" default="false" />
    <aura:attribute name="data4BooleanCheck" type="Boolean" default="false" />
    <aura:attribute name="saveFilterImpl" type="Boolean" default="false"/>
    <aura:attribute name="fieldNameWithRelationship" type="Map"/>
    <aura:attribute name="options" type="List" default="[
                                                        {'label': 'All', 'value': 'And'},
                                                        {'label': 'Any', 'value': 'Or'}
                                                        ]"/>
    <aura:attribute name="resorceId" type="Id" />
    <aura:attribute name="ispercentAllocation" type="Boolean" default="false"/>
    <aura:attribute name="percentAllocationWrapperList" type="Object"/>
    <aura:attribute name="percentageAlloationRow" type = "List" /> 
    <aura:attribute name="ignoreHoliday" type = "Boolean" default = "false"/>
    <aura:attribute name="isUpdated" type="Boolean" default="false"/>
    <aura:attribute name="indexDeletion" type="Integer" default="0"/>
    <aura:attribute name="message" description="Message content, can be HTML"
                    type="String" default="Message from MessageComponent.cmp"/>
    <aura:attribute name="type"
                    description="Message type, can be error/warning/info/success"
                    type="String" default="info"/>
    <aura:attribute name="showError" type="Boolean" default="false" /> 
    <aura:attribute name="disableAssignHoldButton" type="Boolean" default="true" />
    <aura:attribute name="startDateForValidation" type="date" />
    <aura:attribute name="endDateForValidation" type="date" />
    <aura:attribute name="isSendEmail" type="Boolean" default="false" /> 
    <aura:attribute name="resEmail" type="String"/>
    <aura:attribute name="emailSubject" type="String"/>
    <aura:attribute name="emailBody" type="String"/>
    <aura:attribute name="isPastJobs" type="Boolean" default="false" />
    <aura:attribute name="assignmentData" type="Object" />
    <aura:attribute name="assignmentDataColoumns" type="List" />
    <aura:attribute name="isCQ" type="Boolean" default="false" />
    <aura:attribute name="competenceDataList" type="Object" />
    <aura:attribute name="competenceDataColoumns" type="List" />
    <aura:attribute name="qualificationDataList" type="Object" />
    <aura:attribute name="qualificationDataColoumns" type="List" />
    <aura:attribute name="dataLowerLevelDetails" type="Object" />
    <aura:attribute name="columnsLowerLevelDetails" type="List" />
    <aura:attribute name="isLowerLevelDetails" type="Boolean" default="false" />
    <aura:attribute name="conNameLowerLevel" type="String"/>
    <aura:attribute name="accordionAttributeValue" type="String"/>
    <aura:attribute name="accordionSkillValue" type="String"/>
    <aura:attribute name="accordionCertificationValue" type="String"/>
    <aura:attribute name="accordionAdditionalFiltersValue" type="String"/>
    <aura:attribute name="accordionSearchResourceValue" type="String"/>
    <aura:attribute name="accordionRoleValue" type="String"/>
    <aura:attribute name="accordionResourceRequestDetailsValue" type="String"/>
    <aura:attribute name="accordionAddressDetailsValue" type="String"/>
    <aura:attribute name="isRenderRDC" type="Boolean" default="false"/>
    <aura:attribute name="searchedSkillName" type="String"/>
    <aura:attribute name="searchedCertificationName" type="String"/>
    <!--Resource Search Header:Start-->
    <article class="slds-card slds-m-bottom_medium">
        <div class="slds-card__header slds-grid">
            <header class="slds-media slds-media_center slds-has-flexi-truncate">
                <div class="slds-media__figure">
                    <span class="slds-icon_container" title="Search Resource">
                        <lightning:icon iconName="standard:search" alternativeText="search" title="search" />
                        <span class="slds-assistive-text">Resource Search</span>
                    </span>
                </div>
                <div class="slds-media__body">
                    <h2 class="slds-card__header-title">
                        <span>Search Resources</span>
                    </h2>
                </div>
            </header>
        </div>
    </article>
    <!--Resource Search Header:End-->
    <div class="container slds-page-header slds-box slds-theme_default slds-theme_backgroundColor-white slds-p-around_none">
        <!--Buttons:Start-->
        <div class="slds-theme_shade slds-p-around_small">
            
            <lightning:buttonIcon class="slds-button slds-button--brand" iconName="utility:filterList" variant="brand" alternativeText="Filter" title="Filter"
                                  onclick="{! c.openFilterWindow }" />
            <lightning:button class="slds-button slds-button--brand" variant="brand" label="Save Filter" title="Save Filter" onclick="{!c.saveFilter}"/>            
            <lightning:button class="slds-button slds-button--brand" variant="brand" label="Submit For Approval" title="Submit For Approval" onclick="{!c.submitForPDMApproval}"/> 
            <aura:if isTrue="{!v.isLowerLevelDetails}">
                <lightning:button class="slds-button slds-button--brand" variant="brand" label="Overview" title="Overview" onclick="{!c.showUpperLevelDetails}"/>
                <aura:set attribute="else">
                    <lightning:button class="slds-button slds-button--brand" variant="brand" label="Lower Level Details" title="Lower Level Details" onclick="{!c.showLowerLevelDetails2}"/>
                </aura:set>
            </aura:if>
            <aura:if isTrue="{!v.searchDetailObject.openFromResourceReqScreen}">
                <lightning:button class="slds-button slds-button--brand" variant="brand" label="Back" title="Back" onclick="{!c.backToResourceRequest}"/>
            </aura:if>
        </div>
        <!--Buttons:End-->
        <div class="contentWrapper">
            <!--Filter Panel:Start-->
            <div class="slds-scrollable filterWindowCollapse" aura:id="filterPanel">
                <div class="slds-theme_shade fullView">
                    <div class="slds-grid slds-p-top_small">
                        <div class="slds-col slds-size_5-of-12 slds-p-horizontal_medium slds-text-heading_large">
                            <span>Filter</span>
                        </div>
                        <div class="slds-col slds-size_7-of-12 ">
                            
                            <div class="slds-float_right slds-p-right_xx-small">
                                <lightning:button class="slds-button slds-button--brand" variant="brand" title="Apply" onclick="{!c.searchOutResources}"
                                                  label="Apply" />
                                <lightning:button class="slds-button slds-button--brand" variant="brand" title="Reset" onclick="{!c.resetTheValues}"
                                                  label="Reset" />
                                
                                <lightning:buttonIcon  class="slds-button slds-button--brand" iconName="utility:close" size="medium"
                                                      alternativeText="close" title="Close Window" onclick="{! c.closeFilterWindow }" />
                            </div>
                        </div>
                        <!--div class="slds-col slds-size_2-of-12 slds-p-left_x-small">
                        </div-->
                    </div>
                    <hr class="thin" />
                    <lightning:accordion class="slds-theme_shade" aura:id="accordion" allowMultipleSectionsOpen="true">
                        <aura:if isTrue="{!v.searchDetailObject.openFromResourceReqScreen}">
                            <lightning:accordionSection name="{!v.accordionResourceRequestDetailsValue}"
                                                        label="{!v.accordionResourceRequestDetailsValue}">
                                <aura:set attribute="body">
                                    <div class="slds-form" role="list">
                                        <!--Resource request Name-->
                                        <div class="slds-form__row">
                                            <div class="slds-form__item" role="listitem">
                                                <div
                                                     class="slds-form-element slds-form-element_edit slds-form-element_horizontal slds-hint-parent">
                                                    <span
                                                          class="slds-form-element__label slds-form-element__static">{!v.searchDetailObject.selectedResourceRequest.Name}</span>
                                                </div>
                                            </div>
                                        </div>
                                        <!--Resource Status-->
                                        <div class="slds-form__row">
                                            <div class="slds-form__item" role="listitem">
                                                <div
                                                     class="slds-form-element slds-form-element_edit slds-form-element_horizontal slds-hint-parent">
                                                    <span class="slds-form-element__label">Status</span>
                                                    <div class="slds-form-element__control">
                                                        <div class="slds-form-element__static">
                                                            <div class="slds-form-element__static">
                                                                <lightning:select aura:id="a_opt" label="" onchange="{!c.statusChange}" value="{!v.searchDetailObject.selectedResourceRequest.pse__Status__c}">
                                                                    <option value="">--None--</option>
                                                                    <aura:iteration items="{!v.searchDetailObject.statusList}" var="wrap">
                                                                        <option text="{!wrap}" value="{!wrap}" />
                                                                    </aura:iteration>
                                                                </lightning:select>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!--Requested Hour-->
                                        <div class="slds-form__row">
                                            <div class="slds-form__item" role="listitem">
                                                <div
                                                     class="slds-form-element slds-form-element_edit slds-form-element_horizontal slds-hint-parent">
                                                    <span class="slds-form-element__label">Requested Hour</span>
                                                    <div class="slds-form-element__control">
                                                        <div class="slds-form-element__static">
                                                            <div class="slds-form-element__static">
                                                                {!v.searchDetailObject.selectedResourceRequest.pse__SOW_Hours__c}
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!--Requested Bill Rate-->
                                        <div class="slds-form__row">
                                            <div class="slds-form__item" role="listitem">
                                                <div
                                                     class="slds-form-element slds-form-element_edit slds-form-element_horizontal slds-hint-parent">
                                                    <span class="slds-form-element__label">Requested Bill Rate</span>
                                                    <div class="slds-form-element__control">
                                                        <div class="slds-form-element__static">
                                                            <div class="slds-form-element__static">
                                                                {!v.searchDetailObject.selectedResourceRequest.pse__Requested_Bill_Rate__c}
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <!--Project-->
                                        <div class="slds-form__row">
                                            <div class="slds-form__item" role="listitem">
                                                <div
                                                     class="slds-form-element slds-form-element_edit slds-form-element_horizontal slds-hint-parent">
                                                    <span class="slds-form-element__label">Project</span>
                                                    <div class="slds-form-element__control">
                                                        <div class="slds-form-element__static">
                                                            <div class="slds-form-element__static">
                                                                {!v.searchDetailObject.selectedResourceRequest.pse__Project__r.Name}
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <!--Opportunity Name-->
                                        <div class="slds-form__row">
                                            <div class="slds-form__item" role="listitem">
                                                <div
                                                     class="slds-form-element slds-form-element_edit slds-form-element_horizontal slds-hint-parent">
                                                    <span class="slds-form-element__label">Opportunity</span>
                                                    <div class="slds-form-element__control">
                                                        <div class="slds-form-element__static">
                                                            <div class="slds-form-element__static">
                                                                {!v.searchDetailObject.selectedResourceRequest.pse__Opportunity__r.Name}
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </aura:set>
                            </lightning:accordionSection>
                            <aura:set attribute="else">
                                <lightning:accordionSection name="{!v.accordionAddressDetailsValue}" label="{!v.accordionAddressDetailsValue}">
                                    <aura:set attribute="body">
                                        <lightning:inputAddress streetLabel="Street" cityLabel="City"
                                                                countryLabel="Country" provinceLabel="Province" postalCodeLabel="PostalCode"
                                                                street="{!v.searchDetailObject.street}" city="{!v.searchDetailObject.city}"
                                                                country="{!v.searchDetailObject.country}"
                                                                province="{!v.searchDetailObject.state}"
                                                                postalCode="{!v.searchDetailObject.zipCode}" onchange="{!c.addressDetailsChangeCount}"/>
                                    </aura:set>
                                </lightning:accordionSection>
                            </aura:set>
                        </aura:if>
                        <aura:if isTrue="{!v.searchDetailObject.customSettingValue}">
                            <lightning:accordionSection name="{!v.accordionRoleValue}" label="{!v.accordionRoleValue}">
                                <aura:set attribute="body">
                                    <lightning:select aura:id="a_opt" label="" onchange="{!c.roleCount}" value="{!v.searchDetailObject.role}">
                                        <option value="">--None--</option>
                                        <aura:iteration items="{!v.searchDetailObject.roleList}" var="wrap">
                                            <option text="{!wrap}" value="{!wrap}" />
                                        </aura:iteration>
                                    </lightning:select>
                                </aura:set>
                            </lightning:accordionSection>
                        </aura:if>
                        <lightning:accordionSection name="{!v.accordionSearchResourceValue}" label="{!v.accordionSearchResourceValue}">
                            <aura:set attribute="body">
                                <aura:if isTrue="{!v.searchDetailObject.openFromResourceReqScreen}">
                                    <div class="">                  
                                        <lightning:input type="text" value="{!v.searchDetailObject.name}"
                                                         disabled="true" onchange="{!c.nameChangeMethod}"/>
                                        <lightning:input type="date" label="Start Date"
                                                         value="{!v.searchDetailObject.startDate}" onchange="{!c.validateDates}" onclick="{!c.validateDatesClick}"/>
                                        <lightning:input type="date" label="End Date"
                                                         value="{!v.searchDetailObject.endDate}" onchange="{!c.validateDates}" onclick="{!c.validateDatesClick}"/>
                                        <lightning:input type="text" label="Name"
                                                         value="{!v.searchDetailObject.resourceRequestName}" onchange="{!c.resourceRequestNameChangeMethod}"/>
                                    </div>
                                    <aura:set attribute="else">
                                        <div
                                             class="">
                                            <lightning:input type="date" name="input6" label="Start Date"
                                                             value="{!v.searchDetailObject.startDate}" onchange="{!c.validateDates}" onclick="{!c.validateDatesClick}"/>
                                            <lightning:input type="date" name="input7" label="End Date"
                                                             value="{!v.searchDetailObject.endDate}" onchange="{!c.validateDates}" onclick="{!c.validateDatesClick}"/>
                                            <lightning:input type="text" name="input8" label="Name"
                                                             value="{!v.searchDetailObject.resourceRequestName}" onchange="{!c.resourceRequestNameChangeMethod}"/>
                                            <lightning:input type="search" name="input9" label="Resource Request"
                                                             value="{!v.searchDetailObject.name}"
                                                             onclick="{!c.openResourceRequestModal}" onchange="{!c.nameChangeMethod}"/>
                                        </div>
                                    </aura:set>
                                </aura:if>
                            </aura:set>
                        </lightning:accordionSection>
                        <lightning:accordionSection name="{!v.accordionAdditionalFiltersValue}" label="{!v.accordionAdditionalFiltersValue}">
                            <aura:set attribute="body">
                                <!--Region-->
                                <div class="slds-grid slds-gutters">
                                    
                                    <div class="slds-col slds-size_2-of-12 slds-m-top_medium">
                                        <span class="slds-form-element__label">Region</span>
                                    </div>
                                    
                                    <div class="slds-col slds-size_5-of-12">
                                        <lightning:select aura:id="regionFilter" label="" value="{!v.searchDetailObject.regionFilter}" onchange="{!c.regionDisabledValueCheck}">
                                            <option value="">--</option>
                                            <option value="Is Null">Is Null</option>
                                            <option value="Not Is Null">Not Is Null</option>
                                            <option value="Equals">Equals</option>
                                            <option value="Not Equals">Not Equals</option>
                                        </lightning:select>
                                    </div>
                                    <div class="slds-col slds-size_5-of-12">
                                        <aura:if isTrue="{!v.regionDisabled}">
                                            <lightning:input type="search"
                                                             value="{!v.searchDetailObject.regionValue}"
                                                             disabled="{!v.regionDisabled}"></lightning:input>
                                            <aura:set attribute="else">
                                                <lightning:input type="search"
                                                                 value="{!v.searchDetailObject.regionValue}"
                                                                 onclick="{!c.openRegionModal}"
                                                                 ></lightning:input>
                                            </aura:set>
                                        </aura:if>
                                    </div>
                                </div>
                                <!--Practice-->
                                <div class="slds-grid slds-gutters">
                                    <div class="slds-col slds-size_2-of-12 slds-m-top_medium">
                                        <span class="slds-form-element__label">Practice</span>
                                    </div>
                                    
                                    <div class="slds-col slds-size_5-of-12">
                                        <lightning:select aura:id="practiceFilter" label="" onchange="{!c.practiceDisabledValueCheck}"
                                                          value="{!v.searchDetailObject.practiceFilter}">
                                            <option value="">--</option>
                                            <option value="Is Null">Is Null</option>
                                            <option value="Not Is Null">Not Is Null</option>
                                            <option value="Equals">Equals</option>
                                            <option value="Not Equals">Not Equals</option>
                                        </lightning:select>
                                    </div>
                                    <div class="slds-col slds-size_5-of-12">
                                        <aura:if isTrue="{!v.practiceDisabled}">
                                            <lightning:input type="search" value="{!v.searchDetailObject.practiceValue}"
                                                             disabled="{!v.practiceDisabled}"
                                                             ></lightning:input>
                                            <aura:set attribute="else">
                                                <lightning:input type="search" value="{!v.searchDetailObject.practiceValue}"
                                                                 onclick="{!c.openPracticeModal}"
                                                                 ></lightning:input>
                                            </aura:set>
                                        </aura:if>              
                                    </div>
                                </div>
                                <!--Group-->
                                <div class="slds-grid slds-gutters">
                                    <div class="slds-col slds-size_2-of-12 slds-m-top_medium">
                                        <span class="slds-form-element__label">Group</span>
                                    </div>
                                    <div class="slds-col slds-size_5-of-12">
                                        <lightning:select aura:id="groupFilter" label="" onchange="{!c.groupDisabledValueCheck}"
                                                          value="{!v.searchDetailObject.groupFilter}">
                                            <option value="">--</option>
                                            <option value="Is Null">Is Null</option>
                                            <option value="Not Is Null">Not Is Null</option>
                                            <option value="Equals">Equals</option>
                                            <option value="Not Equals">Not Equals</option>
                                        </lightning:select>
                                    </div>
                                    <div class="slds-col slds-size_5-of-12">
                                        <aura:if isTrue="{!v.groupDisabled}">
                                            <lightning:input type="search" value="{!v.searchDetailObject.groupValue}"
                                                             disabled="{!v.groupDisabled}"
                                                             ></lightning:input>
                                            <aura:set attribute="else">
                                                <lightning:input type="search" value="{!v.searchDetailObject.groupValue}"
                                                                 onclick="{!c.openGroupModal}"
                                                                 ></lightning:input>
                                            </aura:set>
                                        </aura:if>
                                    </div>
                                </div>
                                <!--Cost Rate-->
                                <div class="slds-grid slds-gutters">
                                    <div class="slds-col slds-size_2-of-12 slds-m-top_medium">
                                        <span class="slds-form-element__label">Cost Rate</span>
                                    </div>
                                    
                                    <div class="slds-col slds-size_5-of-12">
                                        <lightning:select aura:id="costFilter" label="" onchange="{!c.costDisabledValueCheck}"
