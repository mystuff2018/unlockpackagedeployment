    </div>
    <!--Modal Ends For Group-->
    <!--Modal Start For PercentAllocation-->
    <div class="slds-m-around_xx-large right">
        <aura:if isTrue="{!v.ispercentAllocation}">
            <!-- Modal/Popup Box starts here-->
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true"
                     aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                <div class="slds-modal__container">
                    <!-- Modal/Popup Box Header Starts here-->
                    <aura:if isTrue="{!v.assignValue}">
                        <header class="slds-modal__header">
                            <lightning:buttonIcon iconName="utility:close" onclick="{! c.closeModel }"
                                                  alternativeText="Close" variant="bare-inverse" class="slds-modal__close" />
                            <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">
                                Assign {!v.conName}
                            </h2>
                        </header>
                    </aura:if>
                    <aura:if isTrue="{!v.holdValue}">
                        <header class="slds-modal__header">
                            <lightning:buttonIcon iconName="utility:close" onclick="{! c.closeModel }"
                                                  alternativeText="Close" variant="bare-inverse" class="slds-modal__close" />
                            <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">
                                Hold {!v.conName}
                            </h2>
                        </header>
                    </aura:if>
                    <!--Modal/Popup Box Body Starts here-->
                    <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                        <div class="slds-form__row">
                            <div class="slds-form__item" role="listitem">                             
                            </div>
                        </div>
                        <!--Start-->
                        <div class="slds-float_left slds-p-right_xx-small">
                            <lightning:button class="slds-button slds-button--brand" variant="brand" label="Add Date Range" title="Add Date Range" onclick="{!c.addDateRangePercentAllocation}"
                                              />
                            <lightning:button class="slds-button slds-button--brand" variant="brand" label="Update" title="Update" onclick="{!c.updatePercentAllocation}"
                                              />
                            <lightning:button class="slds-button slds-button--brand" variant="brand" label="Remove" title="Remove" onclick="{!c.deleteParticularRowBasedOnPercentAllocation}"
                                              />
                            <lightning:input type="checkbox" label="Ignore Holiday" auraId="checkBox" checked="{!v.ignoreHoliday}" />
                        </div>
                        &nbsp;&nbsp;
                        <!--End-->
                        
                        <div class="table-container">
                            <table style = "overflow: scroll;overflow: auto">
                                <thead>
                                    <tr class="slds-line-height_reset">
                                        <th  scope="col">
                                            <div class="slds-truncate" style = "width:20px" title="Sunday"></div>
                                        </th>
                                        <th  scope="col">
                                            <div class="slds-truncate" style = "width:120px" title="Sunday">Start Date</div>
                                        </th>
                                        <th  scope="col">
                                            <div class="slds-truncate" style = "width:120px" title="Sunday">End Date</div>
                                        </th>
                                        <th class="" scope="col">
                                            <div class="slds-truncate" title="Sunday">Allocation</div>
                                        </th>
                                        <th class="" scope="col">
                                            <div class="slds-truncate" title="Sunday">Sunday</div>
                                        </th>
                                        <th class="" scope="col">
                                            <div class="slds-truncate" title="Monday">Monday</div>
                                        </th>
                                        <th class="" scope="col">
                                            <div class="slds-truncate" title="Tuesday">Tuesday</div>
                                        </th>
                                        <th class="" scope="col">
                                            <div class="slds-truncate" title="Wednesday">Wednesday</div>
                                        </th>
                                        <th class="" scope="col">
                                            <div class="slds-truncate" title="Thursday">Thursday</div>
                                        </th>
                                        <th class="" scope="col">
                                            <div class="slds-truncate" title="Friday">Friday</div>
                                        </th>
                                        <th class="" scope="col">
                                            <div class="slds-truncate" title="Saturday">Saturday</div>
                                        </th>
                                        <th class="" scope="col">
                                            <div class="slds-truncate" title="Total">Total</div>
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <aura:iteration items="{!v.percentageAlloationRow}" var="row">
                                        <tr class="slds-hint-parent">
                                            <td >
                                                <div class="slds-truncate" style = "width:20px">
                                                    <ui:inputCheckbox name="{!row.indexForDeletion}"
                                                                      value="{!row.isSelectedCheck}"/>                                                    
                                                </div>
                                            </td> 
                                            <td >
                                                <div class="slds-truncate" style = "width:120px">
                                                    <lightning:input type="date"                          
                                                                     value="{!row.startDate}" />
                                                </div>
                                            </td>
                                            <td >
                                                <div class="slds-truncate" style = "width:120px">
                                                    <lightning:input type="date"
                                                                     value="{!row.endDate}" />
                                                </div>
                                            </td>
                                            <td>
                                                <div class="slds-truncate" style = "width:60px">
                                                    <lightning:input type="Double"
                                                                     value="{!row.percentageAllocation}" />
                                                </div>
                                            </td>
                                            <td>
                                                <div class="slds-truncate">
                                                    {!row.schedule.pse__Sunday_Hours__c}
                                                </div>
                                            </td>
                                            <td>
                                                <div class="slds-truncate">
                                                    {!row.schedule.pse__Monday_Hours__c}
                                                </div>
                                            </td>
                                            <td>
                                                <div class="slds-truncate">
                                                    {!row.schedule.pse__Tuesday_Hours__c}
                                                </div>
                                            </td>
                                            <td>
                                                <div class="slds-truncate">
                                                    {!row.schedule.pse__Wednesday_Hours__c}
                                                </div>
                                            </td>
                                            <td>
                                                <div class="slds-truncate">
                                                    {!row.schedule.pse__Thursday_Hours__c}
                                                </div>
                                            </td>
                                            <td>
                                                <div class="slds-truncate">
                                                    {!row.schedule.pse__Friday_Hours__c}
                                                </div>
                                            </td>
                                            <td>
                                                <div class="slds-truncate">
                                                    {!row.schedule.pse__Saturday_Hours__c}
                                                </div>
                                            </td>
                                            <td>
                                                <div class="slds-truncate">{!row.totalHours}</div>
                                            </td>
                                        </tr>
                                    </aura:iteration>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <aura:if isTrue="{!v.showError}">
                        <div class="{!'slds-box slds-theme–alert-texture slds-theme–' + v.type}" style="background-color: coral">
                            <lightning:icon iconName="{! 'utility:'+ v.type }" size="medium"
                                            
                                            alternativeText="{!v.type}" Class="iconContainer"/>&nbsp;
                            <span class="slds-text-body–regular">
                                <aura:unescapedHtml value="{!v.message}"/>
                            </span>
                        </div>
                    </aura:if>
                    <!--Modal/Popup Box Footer Starts here-->
                    <aura:if isTrue="{!v.assignValue}">
                        <footer class="slds-modal__footer">
                            <lightning:button variant="brand" label="Assign" title="Assign"
                                              onclick="{! c.partialAssign }" disabled="{!v.disableAssignHoldButton}"/>
                            <lightning:button variant="brand" label="Close" title="Close" onclick="{! c.closeModel }" />
                        </footer>
                    </aura:if>
                    <aura:if isTrue="{!v.holdValue}">
                        <footer class="slds-modal__footer">
                            <lightning:button variant="brand" label="Hold" title="Hold"
                                              onclick="{! c.partialAssign }" disabled="{!v.disableAssignHoldButton}"/>
                            <lightning:button variant="brand" label="Close" title="Close" onclick="{! c.closeModel }" />
                        </footer>
                    </aura:if>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </aura:if>
    </div>
    <!--Modal Ends For PercentAllocation-->
    <!--Modal Start For SendEmail-->
    <div class="slds-m-around_xx-large right">
        <aura:if isTrue="{!v.isSendEmail}">
            <!-- Modal/Popup Box starts here-->
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true"
                     aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                <div class="slds-modal__container">
                    <!-- Modal/Popup Box Header Starts here-->
                    <header class="slds-modal__header">
                        <lightning:buttonIcon iconName="utility:close" onclick="{! c.closeModel }"
                                              alternativeText="Close" variant="bare-inverse" class="slds-modal__close" />
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">
                            Send Email
                        </h2>
                    </header>
                    <!--Modal/Popup Box Body Starts here-->
                    <div class="slds-modal__content slds-p-around_medium">
                        <lightning:input aura:id="emailAddress" label="To" value="{!v.resEmail}"/>
                        <lightning:input aura:id="emailSubject" label="Subject" value="{!v.emailSubject}"/>
                        <lightning:textArea aura:id="emailBody" label="Body" value="{!v.emailBody}"/>
                    </div>
                    <!--Modal/Popup Box Footer Starts here-->
                    <footer class="slds-modal__footer">
                        <lightning:button variant="brand" label="Send" title="Send" onclick="{!c.sendEmailToResource}" />
                        <lightning:button variant="brand" label="Close" title="Close" onclick="{!c.closeModel}" />
                    </footer>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </aura:if>
    </div>
    <!--Modal Ends For SendEmail-->
    <!--Modal Start For PastJobs-->
    <div class="slds-m-around_xx-large right">
        <aura:if isTrue="{!v.isPastJobs}">
            <!-- Modal/Popup Box starts here-->
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true"
                     aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                <div class="slds-modal__container">
                    <!-- Modal/Popup Box Header Starts here-->
                    <header class="slds-modal__header">
                        <lightning:buttonIcon iconName="utility:close" onclick="{! c.closeModel }"
                                              alternativeText="Close" variant="bare-inverse" class="slds-modal__close" />
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">
                            Past Jobs
                        </h2>
                    </header>
                    <!--Modal/Popup Box Body Starts here-->
                    <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                        <lightning:datatable columns="{! v.assignmentDataColoumns }" data="{! v.assignmentData }" hideCheckboxColumn="true" keyField="id"
                                             />
                    </div>
                    <!--Modal/Popup Box Footer Starts here-->
                    <footer class="slds-modal__footer">
                        <lightning:button variant="brand" label="Close" title="Close" onclick="{! c.closeModel }" />
                    </footer>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </aura:if>
    </div>
    <!--Modal Ends For PastJobs-->
    <!--Modal Start For CQ-->
    <div class="slds-m-around_xx-large right">
        <aura:if isTrue="{!v.isCQ}">
            <!-- Modal/Popup Box starts here-->
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true"
                     aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                <div class="slds-modal__container">
                    <!-- Modal/Popup Box Header Starts here-->
                    <header class="slds-modal__header">
                        <lightning:buttonIcon iconName="utility:close" onclick="{! c.closeModel }"
                                              alternativeText="Close" variant="bare-inverse" class="slds-modal__close" />
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">
                            Skills and Certifications
                        </h2>
                    </header>
                    <!--Modal/Popup Box Body Starts here-->
                    <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                        <div class="slds-text-align_center slds-text-heading_large">{!v.conNameLowerLevel}</div>
                        <lightning:accordion class="slds-theme_shade" aura:id="accordionCQ" allowMultipleSectionsOpen="true">
                            <lightning:accordionSection name="Skills"
                                                        label="Skills">
                                <aura:set attribute="body">
                                    <aura:if isTrue="{!not(empty(v.competenceDataList))}">
                                        <lightning:datatable columns="{! v.competenceDataColoumns }" data="{! v.competenceDataList }" hideCheckboxColumn="true" keyField="id"
                                                             />
                                        <aura:set attribute="else">
                                            <p>No Skills found for the selected resource.</p>
                                        </aura:set>
                                    </aura:if>
                                </aura:set>
                            </lightning:accordionSection>
                            <lightning:accordionSection name="Certifications"
                                                        label="Certifications">
                                <aura:set attribute="body">
                                    <aura:if isTrue="{!not(empty(v.qualificationDataList))}">
                                        <lightning:datatable columns="{! v.qualificationDataColoumns }" data="{! v.qualificationDataList }" hideCheckboxColumn="true" keyField="id"
                                                             />
                                        <aura:set attribute="else">
                                            <p>No Certifications found for the selected resource.</p>
                                        </aura:set>
                                    </aura:if>
                                </aura:set>
                            </lightning:accordionSection>
                        </lightning:accordion>
                    </div>
                    <!--Modal/Popup Box Footer Starts here-->
                    <footer class="slds-modal__footer">
                        <lightning:button variant="brand" label="Close" title="Close" onclick="{! c.closeModel }" />
                    </footer>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </aura:if>
    </div>
    <!--Modal Ends For CQ-->
    <!--Code Start For Spinner-->
    <aura:if isTrue="{!v.spinner}">
        <div role="dialog" tabindex="-1" class="slds-modal slds-fade-in-open">
            <div aura:id="spinnerId" class="slds-spinner_container">
                <div class="slds-spinner--brand slds-spinner slds-spinner--large slds-is-relative" role="alert">
                    <span class="slds-assistive-text">Loading...</span>
                    <div class="slds-spinner__dot-a"></div>
                    <div class="slds-spinner__dot-b"></div>
                </div>
            </div>
        </div>
    </aura:if>
    <!--Code Ends For Spinner-->
</aura:component>